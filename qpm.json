{
  "sharedDir": "shared",
  "dependenciesDir": "extern",
  "info": {
    "name": "BSML",
    "id": "bsml",
    "version": "0.5.0",
    "url": "https://github.com/RedBrumbler/Quest-BSML",
    "additionalData": {
      "overrideSoName": "libbsml.so",
      "cmake": true
    }
  },
  "dependencies": [
    {
      "id": "libil2cpp",
      "versionRange": "^0.3.0",
      "additionalData": {}
    },
    {
      "id": "beatsaber-hook",
      "versionRange": "^5.1.5",
      "additionalData": {}
    },
    {
      "id": "bs-cordl",
      "versionRange": "3500.*",
      "additionalData": {}
    },
    {
      "id": "custom-types",
      "versionRange": "^0.17.4",
      "additionalData": {}
    },
    {
      "id": "scotland2",
      "versionRange": "^0.1.4",
      "additionalData": {
        "includeQmod": false,
        "private": true
      }
    },
    {
      "id": "paper",
      "versionRange": "^3.0.0",
      "additionalData": {
        "private": true
      }
    },
    {
      "id": "tinyxml2",
      "versionRange": "^10.0.0",
      "additionalData": {}
    },
    {
      "id": "gif-lib",
      "versionRange": "^5.2.2",
      "additionalData": {
        "private": true
      }
    },
    {
      "id": "kaleb",
      "versionRange": "^0.1.6",
      "additionalData": {
        "private": true
      }
    }
  ],
  "workspace": {
    "scripts": {
      "build": [
        "pwsh ./scripts/build.ps1"
      ],
      "clean": [
        "pwsh ./scripts/build.ps1 -clean"
      ],
      "copy": [
        "pwsh ./scripts/copy.ps1"
      ],
      "log": [
        "pwsh ./scripts/log.ps1"
      ],
      "qmod": [
        "pwsh ./scripts/build.ps1 -clean",
        "qpm qmod build --isLibrary true",
        "pwsh ./scripts/createqmod.ps1 BSML -clean"
      ]
    }
  }
}
